	<queryString>
		<![CDATA[SELECT
     sales_delivery.`id` AS sales_delivery_id,
     sales_delivery_item.`qty` AS sales_delivery_item_qty,
     product_unit.`identifier` AS product_unit_identifier,
     product.`identifier` AS product_identifier,
     product.`description` AS product_description,
     sales_delivery_item.`price` AS sales_delivery_item_price,
     customer.`name` AS customer_name,
     customer.`bil_addr_street` AS customer_bil_addr_street,
     city.`description` AS city_description,
     province.`description` AS province_description,
     region.`description` AS region_description,
     customer.`bil_addr_zip` AS customer_bil_addr_zip,
     country.`description` AS country_description,
     sales_delivery.`date` AS sales_delivery_date,
     sales_agent.`last_name` AS sales_agent_last_name,
     sales_agent.`first_name` AS sales_agent_first_name,
     sales_delivery.`discount` AS sales_delivery_discount,
     sales_delivery.`term_day` AS sales_delivery_term_day,
     discount_group.`identifier` AS discount_group_identifier
FROM
     `sales_delivery` sales_delivery INNER JOIN `sales_delivery_item` sales_delivery_item ON sales_delivery.`id` = sales_delivery_item.`delivery_id`
     LEFT JOIN `product` product ON sales_delivery_item.`product_id` = product.`id`
     LEFT JOIN `product_unit` product_unit ON product.`unit_id` = product_unit.`id`
     LEFT JOIN `customer` customer ON sales_delivery.`customer_id` = customer.`id`
     LEFT JOIN `sales_agent` sales_agent ON sales_delivery.`sales_agent_id` = sales_agent.`id`
     LEFT JOIN `discount_group` discount_group ON sales_delivery.`discount_group_id` = discount_group.`id`
     LEFT JOIN `city` city ON customer.`bil_addr_city_id` = city.`id`
     LEFT JOIN `province` province ON city.`province_id` = province.`id`
     LEFT JOIN `region` region ON province.`region_id` = region.`id`
     LEFT JOIN `country` country ON region.`country_id` = country.`id`
WHERE
     sales_delivery.`id` = $P{id}]]>
