	<queryString>
		<![CDATA[SELECT
     stock_transfer.`id` AS stock_transfer_id,
     stock_transfer.`date` AS stock_transfer_date,
     stock_transfer_item.`qty` AS stock_transfer_item_qty,
     product_unit.`identifier` AS product_unit_identifier,
     product.`identifier` AS product_identifier,
     product.`description` AS product_description,
     stock_transfer.`received_by` AS stock_transfer_received_by,
     stock_transfer.`prepared_by` AS stock_transfer_prepared_by

FROM
     `stock_transfer` stock_transfer INNER JOIN `stock_transfer_item` stock_transfer_item ON stock_transfer.`id` = stock_transfer_item.`transfer_id`
     INNER JOIN `product` product ON stock_transfer_item.`product_id` = product.`id`
     INNER JOIN `product_unit` product_unit ON product.`unit_id` = product_unit.`id`
WHERE
     stock_transfer.`id` = $P{id}]]>
