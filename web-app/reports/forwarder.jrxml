	<queryString language="SQL">
		<![CDATA[SELECT
     forwarder.`description` AS forwarder_description,
     forwarder.`identifier` AS forwarder_identifier,
     forwarder.`landline` AS forwarder_landline,
     concat(city.`description`,', ',province.`description`,', ',region.`description`,', ',forwarder.`zip`)AS forwarder_address,
     company.`logo` AS company_logo,
     company.`landline` AS company_landline,
     company.`fax` AS company_fax,
     company.`website` AS company_website,
     company.`email` AS company_email,
     company.`name` AS company_name,
     company.`registered_address` AS company_registered_address
FROM
     `city` city INNER JOIN `province` province ON city.`province_id` = province.`id`
     INNER JOIN `region` region ON province.`region_id` = region.`id`
     INNER JOIN `forwarder` forwarder ON city.`id` = forwarder.`city_id`,
     `company` company]]>
	</queryString>
