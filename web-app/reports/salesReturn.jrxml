	<queryString>
		<![CDATA[SELECT
     customer.`name` AS customer_name,
     sales_return.`id` AS sales_return_id,
     sales_return.`date` AS sales_return_date,
     sales_return_item.`qty` AS sales_return_item_qty,
     product_unit.`identifier` AS product_unit_identifier,
     product.`identifier` AS product_identifier,
     product.`description` AS product_description,
     sales_return_item.`price` AS sales_return_item_price,
     sales_return.`remark` AS sales_return_remark,
     sales_return.`discount` AS sales_return_discount
FROM
     `customer` customer INNER JOIN `sales_return` sales_return ON customer.`id` = sales_return.`customer_id`
     INNER JOIN `sales_return_item` sales_return_item ON sales_return.`id` = sales_return_item.`sales_return_id`
     INNER JOIN `product` product ON sales_return_item.`product_id` = product.`id`
