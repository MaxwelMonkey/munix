	<queryString>
		<![CDATA[SELECT
     concat(sales_agent.`last_name`,', ',sales_agent.`first_name`)AS sales_agent_name,
     sales_agent.`landline` AS sales_agent_landline,
     sales_agent.`identifier` AS sales_agent_identifier,
     sales_agent.`mobile` AS sales_agent_mobile,
     sales_agent.`skype` AS sales_agent_skype,
     sales_agent.`yahoo` AS sales_agent_yahoo,
     concat(sales_agent.`street`,', ',city.`description`,', ',province.`description`,', ',region.`description`,', ',sales_agent.`zip`)AS sales_agent_address,
     company.`logo` AS company_logo,
     company.`name` AS company_name,
     company.`registered_address` AS company_registered_address,
     company.`landline` AS company_landline,
     company.`fax` AS company_fax,
     company.`website` AS company_website,
     company.`email` AS company_email
FROM
     `city` city INNER JOIN `sales_agent` sales_agent ON city.`id` = sales_agent.`city_id`
     INNER JOIN `province` province ON city.`province_id` = province.`id`
     INNER JOIN `region` region ON province.`region_id` = region.`id`,
     `company` company]]>
	</queryString>
