	<queryString>
		<![CDATA[SELECT
     sales_replacement.`id` AS sales_replacement_id,
     sales_replacement.`date` AS sales_replacement_date,
     customer.`name` AS customer_name,
     sales_replacement_item.`qty` AS sales_replacement_item_qty,
     product_unit.`identifier` AS product_unit_identifier,
     product.`identifier` AS product_identifier,
     product.`description` AS product_description,
     sales_replacement.`remark` AS sales_replacement_remark,
     sales_replacement.`prepared_by` AS sales_replacement_prepared_by,
     sales_replacement.`sales_return_id` AS sales_replacement_sales_return_id
FROM
     `customer` customer INNER JOIN `sales_replacement` sales_replacement ON customer.`id` = sales_replacement.`customer_id`
     INNER JOIN `sales_replacement_item` sales_replacement_item ON sales_replacement.`id` = sales_replacement_item.`sales_replacement_id`
     INNER JOIN `product` product ON sales_replacement_item.`product_id` = product.`id`
     INNER JOIN `product_unit` product_unit ON product.`unit_id` = product_unit.`id`
WHERE
     sales_replacement.`id` = $P{id}]]>
