	<queryString>
		<![CDATA[SELECT
     job_order.`id` AS job_order_id,
     job_order.`start_date` AS job_order_start_date,
     job_order.`target_date` AS job_order_target_date,
     product.`identifier` AS product_identifier,
     product.`description` AS product_description,
     assemble.`identifier` AS assemble_identifier,
     job_order.`qty` AS job_order_qty,
     material_requisition_item.`qty` AS material_requisition_item_qty,
     product_A.`identifier` AS product_A_identifier,
     product_A.`description` AS product_A_description,
     product_unit.`identifier` AS product_unit_identifier
FROM
     `product` product INNER JOIN `job_order` job_order ON product.`id` = job_order.`product_id`
     INNER JOIN `assemble` assemble ON job_order.`assigned_to_id` = assemble.`id`
     INNER JOIN `material_requisition` material_requisition ON job_order.`id` = material_requisition.`job_order_id`
     INNER JOIN `material_requisition_item` material_requisition_item ON material_requisition.`id` = material_requisition_item.`requisition_id`
     INNER JOIN `product` product_A ON material_requisition_item.`component_id` = product_A.`id`
     INNER JOIN `product_unit` product_unit ON product_A.`unit_id` = product_unit.`id`
WHERE
     job_order.`id` = $P{id}]]>
	</queryString>
