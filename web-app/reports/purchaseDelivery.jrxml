	<queryString>
		<![CDATA[SELECT
     supplier.`name` AS supplier_name,
     purchase_invoice.`reference` AS purchase_invoice_reference,
     purchase_order.`id` AS purchase_order_id,
     purchase_invoice.`date` AS purchase_invoice_date,
     purchase_order.`date` AS purchase_order_date,
     purchase_delivery_item.`qty` AS purchase_delivery_item_qty,
     product_unit.`identifier` AS product_unit_identifier,
     product.`identifier` AS product_identifier,
     product.`description` AS product_description,
     purchase_delivery_item.`price` AS purchase_delivery_item_price,
     purchase_delivery.`prepared_by` AS purchase_delivery_prepared_by,
     company.`logo` AS company_logo,
     company.`landline` AS company_landline,
     company.`fax` AS company_fax,
     company.`website` AS company_website,
     company.`email` AS company_email,
     company.`name` AS company_name,
     company.`registered_address` AS company_registered_address,
     purchase_delivery.`id` AS purchase_delivery_id
FROM
     `supplier` supplier INNER JOIN `purchase_order` purchase_order ON supplier.`id` = purchase_order.`supplier_id`
     INNER JOIN `purchase_delivery` purchase_delivery ON purchase_order.`id` = purchase_delivery.`po_id`
     INNER JOIN `purchase_invoice` purchase_invoice ON purchase_delivery.`purchase_invoice_id` = purchase_invoice.`id`
     INNER JOIN `purchase_delivery_item` purchase_delivery_item ON purchase_delivery.`id` = purchase_delivery_item.`delivery_id`
     INNER JOIN `product` product ON purchase_delivery_item.`product_id` = product.`id`
     INNER JOIN `product_unit` product_unit ON product.`unit_id` = product_unit.`id`,
     `company` company
WHERE
    purchase_delivery.`id` = $P{id}]]>
	</queryString>
