     customer.`fax` AS customer_fax,
     customer.`landline` AS customer_landline,
     customer.`mobile` AS customer_mobile,
     customer.`email` AS customer_email,
     customer.`skype` AS customer_skype,
     customer.`website` AS customer_website,
     company.`logo` AS company_logo,
     company.`landline` AS company_landline,
     company.`fax` AS company_fax,
     company.`website` AS company_website,
     company.`email` AS company_email,
     company.`name` AS company_name,
     company.`registered_address` AS company_registered_address
FROM
     `sales_agent` sales_agent INNER JOIN `customer` customer ON sales_agent.`id` = customer.`sales_agent_id`,
     `company` company]]>
	</queryString>
