	<queryString>
		<![CDATA[SELECT
     supplier.`identifier` AS supplier_identifier,
     supplier.`name` AS supplier_name,
     supplier.`tin` AS supplier_tin,
     supplier.`address` AS supplier_address,
     term.`description` AS term_description,
     currency_type.`identifier` AS currency_type_identifier,
     supplier.`contact` AS supplier_contact,
     supplier.`fax` AS supplier_fax,
     supplier.`landline` AS supplier_landline,
     supplier.`email` AS supplier_email,
     supplier.`website` AS supplier_website,
     supplier.`yahoo` AS supplier_yahoo,
     supplier.`skype` AS supplier_skype,
     company.`logo` AS company_logo,
     company.`landline` AS company_landline,
     company.`fax` AS company_fax,
     company.`website` AS company_website,
     company.`email` AS company_email,
     company.`name` AS company_name,
     company.`registered_address` AS company_registered_address
FROM
     `term` term INNER JOIN `supplier` supplier ON term.`id` = supplier.`term_id`
     INNER JOIN `currency_type` currency_type ON supplier.`currency_id` = currency_type.`id`,
     `company` company]]>
	</queryString>
