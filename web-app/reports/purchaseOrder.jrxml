	<queryString>
		<![CDATA[SELECT
     supplier.`name` AS supplier_name,
     supplier.`address` AS supplier_address,
     purchase_order.`id` AS purchase_order_id,
     purchase_order.`date` AS purchase_order_date,
     purchase_order_item.`qty` AS purchase_order_item_qty,
     product_unit.`identifier` AS product_unit_identifier,
     product.`identifier` AS product_identifier,
     product.`description` AS product_description,
     purchase_order_item.`price` AS purchase_order_item_price,
     purchase_order.`prepared_by` AS purchase_order_prepared_by,
     purchase_order.`approved_by` AS purchase_order_approved_by
FROM
     `purchase_order` purchase_order INNER JOIN `purchase_order_item` purchase_order_item ON purchase_order.`id` = purchase_order_item.`po_id`
     INNER JOIN `product` product ON purchase_order_item.`product_id` = product.`id`
     INNER JOIN `product_unit` product_unit ON product.`unit_id` = product_unit.`id`
     INNER JOIN `supplier` supplier ON purchase_order.`supplier_id` = supplier.`id`
WHERE
     purchase_order.`id` = $P{id}]]>
