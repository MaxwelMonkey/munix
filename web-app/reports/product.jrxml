	<queryString>
		<![CDATA[SELECT
     product.`identifier` AS product_identifier,
     product.`description` AS product_description,
     product_brand.`identifier` AS product_brand_identifier,
     product_category.`identifier` AS product_category_identifier,
     product_color.`identifier` AS product_color_identifier,
     product_material.`identifier` AS product_material_identifier,
     product_model.`identifier` AS product_model_identifier,
     product_size.`identifier` AS product_size_identifier,
     product_subcategory.`identifier` AS product_subcategory_identifier,
     product_type.`identifier` AS product_type_identifier,
     product_unit.`identifier` AS product_unit_identifier,
     product.`series` AS product_series,
     product.`part_number` AS product_part_number,
     stock.`qty` AS stock_qty,
     company.`logo` AS company_logo,
     company.`landline` AS company_landline,
     company.`fax` AS company_fax,
     company.`website` AS company_website,
     company.`email` AS company_email,
     company.`name` AS company_name,
     company.`registered_address` AS company_registered_address
FROM
     `product` product INNER JOIN `product_brand` product_brand ON product.`brand_id` = product_brand.`id`
     INNER JOIN `product_category` product_category ON product.`category_id` = product_category.`id`
     INNER JOIN `product_color` product_color ON product.`color_id` = product_color.`id`
     INNER JOIN `product_material` product_material ON product.`material_id` = product_material.`id`
     INNER JOIN `product_model` product_model ON product.`model_id` = product_model.`id`
     INNER JOIN `product_size` product_size ON product.`size_id` = product_size.`id`
     INNER JOIN `product_subcategory` product_subcategory ON product.`subcategory_id` = product_subcategory.`id`
     INNER JOIN `product_type` product_type ON product.`type_id` = product_type.`id`
     INNER JOIN `product_unit` product_unit ON product.`unit_id` = product_unit.`id`
     INNER JOIN `stock` stock ON product.`id` = stock.`product_id`,
     `company` company]]>
	</queryString>
